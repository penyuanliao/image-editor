{
  "name": "asset-editor",
  "private": true,
  "version": "0.1.36",
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "qatest": "vite --mode qatest",
    "pd": "vite --mode pd",
    "main": "vite --mode main",
    "build": "npm version patch --no-git-tag-version && vue-tsc -b && vite build",
    "build:test": "npm version patch --no-git-tag-version && vue-tsc -b && vite build --mode test",
    "build:pd": "npm version patch --no-git-tag-version && vue-tsc -b && vite build --mode pd",
    "release": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write ./src",
    "upload:s3": "aws s3 cp ./dist \"$npm_config_dest\" --recursive"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@google/genai": "^1.20.0",
    "@gtm-support/vue-gtm": "^3.1.0",
    "@types/node": "^24.9.2",
    "axios": "^1.13.2",
    "colorpickers": "^1.0.4",
    "element-plus": "^2.11.3",
    "fabric": "^6.7.1",
    "gsap": "^3.13.0",
    "konva": "^10.0.2",
    "mitt": "^3.0.1",
    "nanoid": "^5.1.6",
    "pinia": "^3.0.3",
    "svg-path-bbox": "^2.1.0",
    "svg-path-parser": "^1.1.0",
    "vue": "^3.5.21",
    "vue-konva": "^3.2.6",
    "vue-router": "^4.6.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "sass-embedded": "^1.93.2",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vue-tsc": "^3.0.7"
  }
}
